    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.0.2</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-war-plugin</artifactId>
                <version>2.1-beta-1</version>
                <configuration>
                    <failOnMissingWebXml>false</failOnMissingWebXml>
                    <webResources>
                        <resource>
                            <directory>src</directory>
                            <targetPath>WEB-INF</targetPath>
                            <includes>
                                <include>jax-ws-catalog.xml</include>
                                <include>wsdl/**</include>
                            </includes>
                        </resource>
                    </webResources>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>jaxws-maven-plugin</artifactId>
                <version>1.10</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>wsimport</goal>
                        </goals>
                        <configuration>
                            <bindingFiles>
                                <bindingFile>schema-bindings.xml</bindingFile>
                                <bindingFile>wsdl-bindings.xml</bindingFile>
                            </bindingFiles>
                            <wsdlFiles>
                                <wsdlFile>CommercialVehicleCollisionWebserviceImpl.wsdl</wsdlFile>
                            </wsdlFiles>
                            <staleFile>${project.build.directory}/jaxws/stale/cvc.stale</staleFile>
                        </configuration>
                        <id>wsimport-generate-cvc</id>
                        <phase>generate-sources</phase>
                    </execution>
                </executions>
                <dependencies>
                    <dependency>
                        <groupId>javax.xml</groupId>
                        <artifactId>webservices-api</artifactId>
                        <version>1.4</version>
                    </dependency>
                </dependencies>
                <configuration>
                    <bindingDirectory>${basedir}/src/jaxws</bindingDirectory>
                    <sourceDestDir>${project.build.directory}/generated-sources/jaxws-wsimport</sourceDestDir>
                    <xnocompile>true</xnocompile>
                    <verbose>true</verbose>
                    <extension>true</extension>
                    <catalog>${basedir}/src/jax-ws-catalog.xml</catalog>
                </configuration>
            </plugin>
            
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>
                <version>1.2.1</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>exec</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <executable>java</executable>
                    <workingDirectory>.</workingDirectory>
                    <arguments>
                        <argument>-classpath</argument>
                        <classpath />
                        <argument>-Djava.util.logging.config.file=src/main/resources/logging.properties</argument>
                        <!--
                        <argument>-Djavax.net.ssl.keyStore=C:/Projects/gfipm/gfipm-ws/model2/m2wsp/src/main/resources/META-INF/curewspm2-keystore.jks</argument>
                        <argument>-Djavax.net.ssl.keyStorePassword=changeit</argument>
                        <argument>-Djavax.net.ssl.trustStore=C:/Projects/gfipm/gfipm-ws/model2/m2wsc/src/main/resources/META-INF/curewspm2-cacerts.jks</argument>
                        <argument>-Djavax.net.ssl.trustStorePassword=changeit</argument>
                        -->
                        <argument>-Dsun.security.ssl.allowUnsafeRenegotiation=true</argument>
<!--                        <argument>-Djavax.net.debug=ssl</argument> -->
<!--                        <argument>-Dcom.sun.xml.ws.transport.http.client.HttpTransportPipe.dump=true</argument>-->
                        <argument>gov.niem.ws.sample.cvc.service.Validate</argument>
<!--                        <argument>output.xml</argument>-->
                        <argument>test.xml</argument>
                        <argument>uuid-0fa022ae-1598-496a-a19e-2c8a650d2ecd</argument>
                    </arguments>
                </configuration>
            </plugin>            
            
